{
  "name": "Spotify Mute",
  "description": "Spotify extension for muting Ads",
  "version": "0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "spotify.png"
  },
  "content_scripts": [
    {
      "matches": ["https://open.spotify.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "permissions": [""]
}
